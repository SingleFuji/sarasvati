<?xml version="1.0"?>

<!--
  The unit test should progress from A to B to C.
  It should then try to backtrack from C to B and
  then from B to A.
-->
<process-definition name="backtrack-linear" xmlns="http://sarasvati.googlecode.com/ProcessDefinition">

  <node name="A" type="wait" isStart="true">
    <arc to="B"/>
  </node>

  <node name="B" type="wait">
    <arc to ="C"/>
  </node>

  <node name="C" type="wait">
  </node>

</process-definition>