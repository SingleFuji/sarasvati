<?xml version="1.0"?>

<sv:process-definition name="embedded-task-rej"
                       xmlns:sv="http://sarasvati.googlecode.com/ProcessDefinition"
                       xmlns:example="http://sarasvati.googlecode.com/Example">

  <node name="start" isStart="true">
    <arc to="1"/>
  </node>

  <node name="1" type="task">
    <externalArc external="subwf-tasks-rej" instance="1" nodeName="sub1" type="out"/>
    <externalArc external="subwf-tasks-rej" instance="1" nodeName="sub1" type="in" name="reject"/>

    <example:taskName>Enter order</example:taskName>
    <example:taskDesc>
        Enter order into system
    </example:taskDesc>

  </node>

  <node name="end">
    <externalArc external="subwf-tasks-rej" instance="1" nodeName="sub4" type="in"/>
  </node>

</sv:process-definition>